{
  "name": "admin-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn db:types && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "add:component": "npx shadcn-ui@latest add ${NAME} && mv src/ui/theme/ui/${NAME}.tsx src/ui/theme/components && rm -r src/ui/theme/ui && echo \"export * from './\"$NAME\"'\" >> src/ui/theme/components/index.ts && prettier src/ui/theme/components/${NAME}.tsx --write",
    "db:push": "prisma db push",
    "db:types": "prisma generate",
    "db:preview": "prisma studio",
    "auth:key": "openssl rand -base64 32"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@as-integrations/next": "^3.0.0",
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-table": "^8.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "jose": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.292.0",
    "next": "14.0.2",
    "next-themes": "^0.2.1",
    "nextjs-toploader": "^1.6.4",
    "prisma": "^5.5.2",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^1.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
